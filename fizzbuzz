public string FizzBuzz(int num){
    if(num % 3 == 0){
        return "fizz";
    }
    if(num % 5 == 0){
        return "buzz"
    }
    if(num % 3 == 0 && num % 5 == 0){
        return "fizzbuzz";
    }

    return "not fizz, buzz, or fizzbuzz"
}

System.Console.WriteLine(FizzBuzz(3));
System.Console.WriteLine(FizzBuzz(5));
System.Console.WriteLine(FizzBuzz(15));
System.Console.WriteLine(FizzBuzz(4));